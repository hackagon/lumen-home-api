{
  "name": "lumen-home-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "./apps/*",
      "./libs/*"
    ]
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/lumen-home-test/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/lumen-home-test/test/jest-e2e.json",
    "prepare-dev": "husky install",
    "install:aggregation": "yarn && yarn workspace @apps/aggregation-service install",
    "start:dev:aggregation": "yarn start:dev @apps/aggregation-service",
    "start:debug:aggregation": "yarn start:debug @apps/aggregation-service",
    "install:sso": "yarn && yarn workspace @apps/sso-service install",
    "start:dev:sso": "yarn start:dev @apps/sso-service",
    "migration:dev:sso": "yarn workspace @apps/sso-service migration:run",
    "install:booking": "yarn && yarn workspace @apps/booking-service install",
    "start:dev:booking": "yarn start:dev @apps/booking-service",
    "migration:dev:booking": "yarn workspace @apps/booking-service migration:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "async": "^3.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "fast-glob": "^3.2.7",
    "handlebars": "^4.7.7",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "nestjs-typeorm-paginate": "4.0.3",
    "newman": "^5.3.2",
    "newman-reporter-htmlextra": "^1.22.11",
    "nodemailer": "^6.9.3",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.192",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "apps/*.ts": "yarn tsc --noEmit",
    "*.{js,ts}": "yarn run lint",
    "*.{js,jsx,ts}": "yarn prettier --write ."
  }
}
